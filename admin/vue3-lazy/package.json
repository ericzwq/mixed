{
  "name": "vue3-lazy",
  "version": "0.1.0",
  "description": "A lazy loading plugin based on vue3",
  "private": false,
  "main": "lib/index.min.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.ts",
  "keyword": "vue3 lazy load img image lazy-component v-lazy",
  "license": "MIT",
  "author": {
    "name": "eric",
    "email": "1872757047@qq.com"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "lib": "vue-cli-service build --target lib --name vue3-lazy-load --dest lib packages/index.ts",
    "build": "npm run clean && npm run build:esm && npm run build:umd",
    "dev": "npm run clean && npm run dev:umd",
    "build:esm": "rollup  --config build/rollup.esm.config.js",
    "build:umd": "rollup --config build/rollup.umd.config.js",
    "clean": "rimraf ./dist"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@types/lodash-es": "^4.17.4",
    "@vue/cli-service": "^4.5.15",
    "@vue/compiler-sfc": "^3.0.0",
    "element-plus": "^1.3.0-beta.5",
    "postcss-import": "^14.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.5",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-vue": "6.0.0-beta.6"
  },
  "dependencies": {
    "lodash-es": "^4.17.20",
    "postcss": "^8.2.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-postcss": "^4.0.0",
    "tailwindcss": "^1.9.0",
    "typescript": "~3.9.3",
    "vue": "^3.2.27"
  }
}

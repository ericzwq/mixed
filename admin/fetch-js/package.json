{
  "name": "fetch-js",
  "version": "1.0.0",
  "private": false,
  "description": "fetch-based http request plugin",
  "main": "lib/fetch-js.umd.js",
  "module": "lib/fetch-js.esm.js",
  "unpkg": "lib/fetch-js.min.js",
  "typings": "types/index.d.ts",
  "keywords": [
    "http",
    "fetch",
    "request"
  ],
  "license": "MIT",
  "author": {
    "name": "eric",
    "email": "1872757047@qq.com"
  },
  "scripts": {
    "serve": "webpack-dev-server",
    "build": "npm run clean && npm run build:esm && npm run build:umd && npm run build:umd.min",
    "build:esm": "rollup  --config build/rollup.esm.config.js",
    "build:umd": "rollup --config build/rollup.umd.config.js",
    "build:umd.min": "rollup --config build/rollup.umd.min.config.js",
    "clean": "rimraf ./lib && rimraf ./types"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime-corejs2": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "html-webpack-plugin": "^4.5.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-loader": "^7.0.5",
    "typescript": "~4.1.5",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3"
  }
}
